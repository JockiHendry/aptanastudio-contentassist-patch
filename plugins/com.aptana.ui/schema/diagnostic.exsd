<?xml version='1.0' encoding='UTF-8'?>
<!-- Schema file written by PDE -->
<schema targetNamespace="com.aptana.ui" xmlns="http://www.w3.org/2001/XMLSchema">
<annotation>
      <appinfo>
         <meta.schema plugin="com.aptana.ui" id="diagnostic" name="diagnostic"/>
      </appinfo>
      <documentation>
         This extenstion point allows contribution to the output shown from running the &quot;Run Diagnostic Tests...&quot; command.
      </documentation>
   </annotation>

   <element name="extension">
      <annotation>
         <appinfo>
            <meta.element />
         </appinfo>
      </annotation>
      <complexType>
         <sequence minOccurs="1" maxOccurs="unbounded">
            <element ref="diagnosticLog" minOccurs="1" maxOccurs="unbounded"/>
         </sequence>
         <attribute name="point" type="string" use="required">
            <annotation>
               <documentation>
                  a fully qualified identifier of the target extension point
               </documentation>
            </annotation>
         </attribute>
         <attribute name="id" type="string">
            <annotation>
               <documentation>
                  an optional identifier of the extension instance
               </documentation>
            </annotation>
         </attribute>
         <attribute name="name" type="string">
            <annotation>
               <documentation>
                  
               </documentation>
               <appinfo>
                  <meta.attribute translatable="true"/>
               </appinfo>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <element name="diagnosticLog">
      <complexType>
         <attribute name="class" type="string" use="required">
            <annotation>
               <documentation>
                  The class that contributes to what is displayed in the diagnostic logs.  Class must be of type IDiagnosticLog, or it will be omitted.
               </documentation>
               <appinfo>
                  <meta.attribute kind="java" basedOn=":com.aptana.ui.IDiagnosticLog"/>
               </appinfo>
            </annotation>
         </attribute>
         <attribute name="priority" type="string" use="default" value="50">
            <annotation>
               <documentation>
                  The priority is used to sort the diagnostic log entries.
               </documentation>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <annotation>
      <appinfo>
         <meta.section type="since"/>
      </appinfo>
      <documentation>
         This extension point was introduced in Studio 3.0.
      </documentation>
   </annotation>

   <annotation>
      <appinfo>
         <meta.section type="examples"/>
      </appinfo>
      <documentation>
         &lt;p&gt;
&lt;pre&gt;
&lt;extension
  point=&quot;com.my.xyz.diagnostic&quot;&gt;
  &lt;diagnosticLog
    class=&quot;com.my.xyz.MyDiagnosticLog&quot;
    priority=&quot;50&quot;&gt;
  &lt;/diagnosticLog&gt;
&lt;/extension&gt;
&lt;/pre&gt;
&lt;/p&gt;

      </documentation>
   </annotation>

   <annotation>
      <appinfo>
         <meta.section type="apiinfo"/>
      </appinfo>
      <documentation>
         The priority is used to sort the order which each contributed log is shown; a lower number means higher priority.
      </documentation>
   </annotation>



</schema>
