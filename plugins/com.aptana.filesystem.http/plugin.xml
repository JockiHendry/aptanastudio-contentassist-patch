<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.filesystem.filesystems">
      <filesystem
            scheme="http">
         <run
               class="com.aptana.filesystem.http.HttpFileSystem">
         </run>
      </filesystem>
   </extension>
   <extension
         point="org.eclipse.core.filesystem.filesystems">
      <filesystem
            scheme="https">
         <run
               class="com.aptana.filesystem.http.HttpFileSystem">
         </run>
      </filesystem>
   </extension>
</plugin>
